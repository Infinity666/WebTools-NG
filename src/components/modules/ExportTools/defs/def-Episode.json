        "fields": {
            "Media ID":
                {
                    "key": "$.ratingKey",
                    "call": 1,
                    "type": "string"
                },
            "Series Title":
                {
                    "key": "$.grandparentTitle",
                    "call": 1,
                    "type": "string"                    
                },
            "Sort title":
                {
                    "key": "$.titleSort",
                    "call": 1,
                    "type": "string",
                    "postProcess": true                    
                },
            "Episode Title":
                {
                    "key": "$.title",
                    "call": 1,
                    "type": "string"                    
                },
